<div ng-include src="'themeplates/header.ejs'"></div>
<div class="container-narrow" ng-controller="othersPageCtrl">
	<div class="panel panel-info">
      <div class="panel-heading">
        <h3 class="panel-title">Other</h3>
      </div>
      <div>
        <ul class="list-group">
		    <li ng-repeat="item in items" class="list-group-item">
				<a class="panel-title" ng-click="openBrowser(item.url)"><span aria-hidden="true" class={{getIcon(item.type)}}>{{item.name}}</a>
		    </li>
		</ul>
      </div>
    </div>
</div>
<div ng-include src="'themeplates/footer.ejs'"></div>